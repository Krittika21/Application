@model WebApplication1.Models.TestTypeMapping

@{
    ViewData["Title"] = "Delete";
}

<h2>CONFIRM</h2>

<h3>Are you sure you want to delete this Test?</h3>
<div>

    <hr />
    <form asp-action="Index" asp-route-id="@Model.TestId">
        <input type="hidden" asp-for="ID" />
        <a asp-action="DeleteTest" asp-route-id="@Model.TestId" class="btn btn-default">YES</a> |
        <a asp-action="Index" class="btn btn-default">NO</a>
    </form>
</div>
